<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RF-InvalidHTTPMethod">
    <DisplayName>RF-InvalidHTTPMethod</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <StatusCode>401</StatusCode>
            <Payload contentType="application/json">
                {"code":401, "message":"Invalid HTTP Method"}
            </Payload>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>