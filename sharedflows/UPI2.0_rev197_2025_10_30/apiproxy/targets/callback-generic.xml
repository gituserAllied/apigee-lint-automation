<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="callback-generic">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AM-PrepareCallbackTargetRequest</Name>
            </Step>
            <Step>
                <Name>JS-MapMerchantParameters</Name>
                <Condition>(merchantAuthenticationRequired = "true")</Condition>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>AM-SetSuccessResponse</Name>
                <Condition>(proccode != "UPI014")</Condition>
            </Step>
            <Step>
                <Name>JX-TransformResponse</Name>
                <Condition>(proccode == "UPI014")</Condition>
            </Step>
            <Step>
                <Name>AM-AddCORS</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://mocktarget.apigee.net</URL>
    </HTTPTargetConnection>
</TargetEndpoint>