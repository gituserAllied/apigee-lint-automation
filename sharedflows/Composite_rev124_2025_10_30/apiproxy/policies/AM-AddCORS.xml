    </Remove>
    <Set>
        <Headers>
            <Header name="Access-Control-Allow-Origin">https://{virtualhost.name}.icicibank.com</Header>
            <Header name="Access-Control-Allow-Headers">Access-Control-Allow-Origin, Access-Control-Allow-Headers, Access-Control-Request-Method, Access-Control-Request-Headers, Origin, X-Requested-With, Content-Type, Accept, Signature, Authorization, apikey, x-priority</Header>
            <Header name="Access-Control-Max-Age">3628800</Header>
            <Header name="Access-Control-Allow-Methods">POST, GET </Header>
            <Header name="Access-Control-Request-Method">POST, GET</Header>
            <Header name="Access-Control-Allow-Credentials">true</Header>
            <Header name="Cache-Control">no-store, no-cache, must-revalidate, post-check=0, pre-check=0, max-age=0</Header>
            <Header name="Pragma">no-cache</Header>
            <Header name="Expires">0</Header>
            <Header name="X-XSS-protection">1; mode=block</Header>
            <Header name="X-Content-Type-Options">nosniff</Header>
            <Header name="Referrer-Policy">no-referrer</Header>
            <Header name="Strict-Transport-Security">max-age=31536000; includeSubDomains</Header>
            <Header name="X-Frame-Options">deny</Header>
            <Header name="Content-Security-Policy">script-src 'self'; image-src 'self'; frame-ancestors 'none'; default-src 'self'; object-src 'self'</Header>
        </Headers>
    </Set>
    <Remove>
        <Headers>
            <Header name="Server"/>
            <Header name="evil"/>
            <Header name="X-Forwarded-Host"/>
            <Header name="Set-Cookie"/>
            <Header name="X-ORACLE-DMS-ECID"/>
            <Header name="X-REQUESTED-BY"/>
            <Header name="X-Forwarded-For"/>
            <Header name="X-Powered-By"/>
            <Header name="Etag"/>
            <Header name="x-request-id"/>
            <Header name="X-Nginx-Trace-ID"/>
            <Header name="Origin"/>
            <Header name="X-Forwarded-Proto"/>
            <Header name="Transfer-Encoding"/>
        </Headers>
    </Remove>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>