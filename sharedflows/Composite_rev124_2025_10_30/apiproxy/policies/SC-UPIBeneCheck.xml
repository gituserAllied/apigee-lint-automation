<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ServiceCallout async="false" continueOnError="false" enabled="true" name="SC-UPIBeneCheck">
    <DisplayName>SC-UPIBeneCheck</DisplayName>
    <Properties/>
    <Request clearPayload="true" variable="beneficiaryCheckRequest">
        <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
        <Set>
            <Verb>GET</Verb>
            <Path>/api/v1/local/composite/upiBeneficiaryValidation</Path>
            <Headers>
                <Header name="apikey">{private.internalApiKey}</Header>
                <Header name="queryParams">{upiBeneQuery}</Header>
            </Headers>
        </Set>
    </Request>
    <Response>beneficiaryCheckResponse</Response>
    <LocalTargetConnection>
        <APIProxy>Internal</APIProxy>
        <ProxyEndpoint>default</ProxyEndpoint>
        <Properties>
            <Property name="success.codes">1xx,2xx,3xx,4xx,5xx</Property>
        </Properties>
    </LocalTargetConnection>
</ServiceCallout>