    <DefaultFaultRule name="all">
        <AlwaysEnforce>true</AlwaysEnforce>
        <Step>
            <Name>FC-ErrorHandling</Name>
            <!--<Condition>(developer.app.name != "New_Niveus")</Condition>-->
            <Condition>(verifyapikey.VA-VerifyApiKey.client_id != "wZzpfyZxH90ytS4Xd1SM6Jf2A2iisFpR")</Condition>
        </Step>
        <Step>
            <Name>AM-Add-Security-Headers</Name>
        </Step>
        <Step>
            <Name>FC-Niveus_error_handling</Name>
            <!--<Condition>(developer.app.name == "New_Niveus")</Condition>-->
            <Condition>(verifyapikey.VA-VerifyApiKey.client_id = "wZzpfyZxH90ytS4Xd1SM6Jf2A2iisFpR")</Condition>
        </Step>
        <Step>
            <Name>FC-Encryption</Name>
            <!--<Condition>(request.verb != "OPTIONS") and (developer.app.name == "New_Niveus")</Condition>-->
            <Condition>(verifyapikey.VA-VerifyApiKey.client_id = "wZzpfyZxH90ytS4Xd1SM6Jf2A2iisFpR")</Condition>
        </Step>
        <Step>
            <Name>JS-EscapeXml</Name>
        </Step>
        <Step>
            <Name>JS-LogTimeTakenjs</Name>
        </Step>
        <!--<Step>-->
        <!--    <Name>js-Niveus_Error-handling</Name>-->
        <!--    <Condition>(verifyapikey.VA-VerifyApiKey.client_id = "FIfG3fApAIj4TpGO9Qq82zAMJbnYZ5wa")</Condition>-->
        <!--</Step>-->
    </DefaultFaultRule>
