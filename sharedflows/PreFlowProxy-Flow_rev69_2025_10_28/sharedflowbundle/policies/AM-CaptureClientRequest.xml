<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-CaptureClientRequest">
    <DisplayName>AM-CaptureClientRequest</DisplayName>
    <Properties/>
    <AssignVariable>
        <Name>clientRequestTemp</Name>
        <Template>{request.content}</Template>
    </AssignVariable>
