<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AM-RemoveHeader</Name>
                <Condition>(proxy.basepath == "/outbound/vtis/v2")</Condition>
            </Step>
            <Step>
                <Name>JS-SetTarget</Name>
            </Step>
            <Step>
                <Name>AM-disablePathSuffix</Name>
            </Step>
        </Request>
        <Response/>
    </PreFlow>
    <Flows/>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <HTTPTargetConnection>
        <LoadBalancer>
            <Server name="ICICI_UAT"/>
        </LoadBalancer>
        <Path>{targetPath}</Path>
    </HTTPTargetConnection>
</TargetEndpoint>