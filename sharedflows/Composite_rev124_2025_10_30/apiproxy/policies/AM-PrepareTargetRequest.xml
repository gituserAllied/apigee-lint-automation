<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AM-PrepareTargetRequest">
    <DisplayName>AM-PrepareTargetRequest</DisplayName>
    <Properties/>
    <Set>
        <Payload contentType="application/x-www-form-urlencoded">{calloutRequest}</Payload>
    </Set>
    <Remove>
        <Headers>
            <Header name="apikey"/>
        </Headers>
    </Remove>
    <AssignVariable>
        <Name>target.copy.pathsuffix</Name>
        <Value>false</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="https" type="request"/>
</AssignMessage>