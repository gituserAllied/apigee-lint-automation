            </Step>
            <Step>
                <Name>RF-Encrypted-Schema</Name>
                <Condition>(valid == false)</Condition>
            </Step>
            <Step>
                <Condition>(verifyapikey.VA-VerifyApiKey.enableRateLimit = "true")</Condition>
                <Name>KVM-SpikeArrestLimit</Name>
            </Step>
            <Step>
                <Condition>(verifyapikey.VA-VerifyApiKey.enableRateLimit = "true")</Condition>
                <Name>SA-RateLimit</Name>
            </Step>
            <Step>
                <Name>JS-exchangeBCPG</Name>
                <Condition>(verifyapikey.VA-VerifyApiKey.client_id = "bKCtGQxqj7dCbB4r0l4CX9giu77xTIQ4")</Condition>
                <!--<Condition>(verifyapikey.VA-VerifyApiKey.client_id = "Gw9w0CML56rhO38c6YL4dQthE0BV7Hqd") and (verifyapikey.VA-VerifyApiKey.client_id = "bKCtGQxqj7dCbB4r0l4CX9giu77xTIQ4")</Condition>-->
            </Step>
            <Step>
                <Name>AM-BCGPReq</Name>
                <Condition>(verifyapikey.VA-VerifyApiKey.client_id = "bKCtGQxqj7dCbB4r0l4CX9giu77xTIQ4")</Condition>
                <!--<Condition>(verifyapikey.VA-VerifyApiKey.client_id = "Gw9w0CML56rhO38c6YL4dQthE0BV7Hqd") and (verifyapikey.VA-VerifyApiKey.client_id = "bKCtGQxqj7dCbB4r0l4CX9giu77xTIQ4")</Condition>-->
            </Step>
            <Step>
                <Name>JS-RemovePGPVersion</Name>
                <Condition>(request.verb != "OPTIONS") and (verifyapikey.VA-VerifyApiKey.isInternal != "true")  and (developer.app.name = "gpay-composite")</Condition>
            </Step>
            <Step>
                <Name>KVM-MerchantConfigForPGP</Name>
                <Condition>(request.verb != "OPTIONS") and  (developer.app.name == "gpay-composite")</Condition>
            </Step>
            <Step>
                <Name>JS-SetPGPGpayConfigVairables</Name>
                <Condition>(request.verb != "OPTIONS") and  (developer.app.name == "gpay-composite")</Condition>
            </Step>
            <!--<Step>-->
            <!--    <Name>Flow-Callout-1</Name>-->
            <!--<Condition>(request.verb != "OPTIONS") and  (developer.app.name == "gpay-composite")</Condition>-->
            <!--<Condition>(verifyapikey.VA-VerifyApiKey.client_id = "oMK8ZZUAeym3dDzalBpGIfaydN2MITRq")</Condition>-->
            <!--</Step>-->
            <Step>
                <Name>Java-Callout-1</Name>
                <Condition>(request.verb != "OPTIONS") and  (developer.app.name == "gpay-composite")</Condition>
            </Step>
            <Step>
                <Name>FC-Decryption</Name>
                <Condition>(request.verb != "OPTIONS") and (verifyapikey.VA-VerifyApiKey.isTata == null)and (developer.app.name != "gpay-composite")</Condition>
                <!--<Condition>(request.verb != "OPTIONS") and developer.app.name != "freshgravity"</Condition>-->
            </Step>
            <Step>
                <Name>FC-DecryptionFlowEazypay</Name>
                <Condition>(request.verb != "OPTIONS") and (verifyapikey.VA-VerifyApiKey.isTata =="yes") </Condition>
            </Step>
            <Step>
                <Name>FC-SQL-Injection</Name>
                <Condition>(request.verb != "OPTIONS") and  (developer.app.name != "gpay-composite")</Condition>
            </Step>
            <Step>
                <Name>JS-SchemaValidation</Name>
            </Step>
            <Step>
                <Name>Js-prepare-payload</Name>
            </Step>
            <Step>
                <Name>RF-validateSchema</Name>
                <Condition>(Valid == false)</Condition>
            </Step>
            <!--<Step>-->
            <!--    <Name>JS-SchemaValidation</Name>-->
            <!--</Step>-->
            <!--<Step>-->
            <!--    <Name>RF-Encrypted-Schema</Name>-->
            <!--    <Condition>(Valid == false)</Condition>-->
            <!--</Step>-->
            <!--<Step>-->
            <!--    <Name>Validate_Encrypted_Schema</Name>-->
            <!--</Step>-->
            <!--<Step>-->
            <!--    <Name>JS-SchemaValidation</Name>-->
            <!--</Step>-->
            <Step>
                <Name>FC-ThreatProtection</Name>
            </Step>
            <Step>
                <Name>AM_setDecryptedClientRequestLog</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>JS-AnalyticsLoggingDev</Name>
            </Step>
            <Step>
                <Name>FC-Encryption</Name>
                <Condition>(request.verb != "OPTIONS") and (developer.app.name != "freshgravity") and  (developer.app.name != "gpay-composite")</Condition>
            </Step>
            <Step>
                <Name>FC-PGP</Name>
                <Condition>(request.verb != "OPTIONS") and  (developer.app.name == "gpay-composite")</Condition>
            </Step>
            <Step>
