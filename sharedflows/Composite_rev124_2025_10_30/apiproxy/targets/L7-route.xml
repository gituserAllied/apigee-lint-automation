<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="L7-route">
    <Description/>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>AM_setL7TargetRequestLog</Name>
            </Step>
            <Step>
                <Name>AM-PrepareTargetRequest</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>JS-TargetRouter</Name>
            </Step>
            <Step>
                <Name>XJ-XmlToJson</Name>
                <Condition>convertXmlToJson = true</Condition>
            </Step>
            <Step>
                <Name>EV_TargetResponseLog</Name>
            </Step>
            <Step>
                <Name>AM_TargetResponseContentLog</Name>
            </Step>
            <Step>
                <Name>AM_DefaultTargetResponseLog</Name>
                <Condition>(targetResponseLogPayload == undefined) or (targetResponseLogPayload == "")</Condition>
            </Step>
            <Step>
                <Name>JS-TargetRouter</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <LoadBalancer>
            <Server name="ICICI_PASSTHROUGH_UAT"/>
        </LoadBalancer>
        <Path>{modifiedUrl}</Path>
    </HTTPTargetConnection>
</TargetEndpoint>
